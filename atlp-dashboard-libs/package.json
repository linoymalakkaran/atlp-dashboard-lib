{
  "name": "atlp-dashboard",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "husky": "husky",
    "commitlint": "commitlint",
    "lint-staged": "lint-staged",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-grid-lib": "ng build angular-gridster2",
    "build-dashboard-lib": "ng build atlp-dashboard-lib",
    "test": "ng test",
    "test-grid-lib": "ng test angular-gridster2",
    "lint": "ng lint",
    "build-altp-dashboard": "ng build",
    "build-npm": "ng build angular-gridster2",
    "watch": "ng build --watch --configuration development",
    "deploy": "ng deploy --base-href=/atlp-dashboard/  --no-silent",
    "postinstall": "husky install",
    "build_lib_dashboard_designer": "ng build dashboard-designer --configuration production",
    "npm_pack_dashboard_designer": "cd dist/dashboard-designer && npm pack",
    "package_dashboard_designer": "npm run build_lib_dashboard_designer && npm run npm_pack_dashboard_designer",
    "build_lib_angular_gridster2": "ng build angular-gridster2 --configuration production",
    "npm_pack_angular_gridster2": "cd dist/angular-gridster2 && npm pack",
    "package_angular_gridster2": "npm run build_lib_angular_gridster2 && npm run npm_pack_angular_gridster2",
    "build_lib_atlp_dashboard_lib": "ng build atlp-dashboard-lib --configuration production",
    "npm_pack_atlp_dashboard_lib": "cd dist/atlp-dashboard-lib && npm pack",
    "package_atlp_dashboard_lib": "npm run build_lib_atlp_dashboard_lib && npm run npm_pack_atlp_dashboard_lib",
    "build_lib_ng_dynamic_component": "ng build ng-dynamic-component --configuration production",
    "npm_pack_ng_dynamic_component": "cd dist/ng-dynamic-component && npm pack",
    "package_ng_dynamic_component": "npm run build_lib_ng_dynamic_component && npm run npm_pack_ng_dynamic_component",
    "package:all": "npm run package_angular_gridster2 && npm run package_ng_dynamic_component && npm run package_atlp_dashboard_lib && npm run package_dashboard_designer"
  },
  "description": "atlp dashboard",
  "dependencies": {
    "@angular-architects/module-federation": "^14.3.9",
    "@angular/animations": "14.0.0",
    "@angular/cdk": "14.0.0",
    "@angular/common": "14.0.0",
    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/material": "14.0.0",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/router": "14.0.0",
    "angular-gridster2": "file:dist/angular-gridster2/angular-gridster2-14.0.1.tgz",
    "atlp-dashboard-lib": "file:dist/atlp-dashboard-lib/atlp-dashboard-lib-0.0.1.tgz",
    "bootstrap": "^5.1.3",
    "ng-dynamic-component": "file:dist/ng-dynamic-component/ng-dynamic-component-1.0.0.tgz",
    "ngx-markdown": "13.1.0",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.0",
    "@angular-eslint/builder": "14.0.0-alpha.0",
    "@angular-eslint/eslint-plugin": "14.0.0-alpha.0",
    "@angular-eslint/eslint-plugin-template": "14.0.0-alpha.0",
    "@angular-eslint/schematics": "14.0.0-alpha.0",
    "@angular-eslint/template-parser": "14.0.0-alpha.0",
    "@angular/cli": "14.0.0",
    "@angular/compiler-cli": "14.0.0",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@types/jasmine": "4.0.3",
    "@types/node": "17.0.40",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "angular-cli-ghpages": "1.0.0",
    "eslint": "8.17.0",
    "husky": "8.0.1",
    "jasmine-core": "4.1.1",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.1",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "13.0.0",
    "ng-packagr": "14.0.0",
    "prettier": "2.6.2",
    "typescript": "4.7.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{html,scss,js,ts,md}": [
      "prettier --write"
    ]
  }
}
